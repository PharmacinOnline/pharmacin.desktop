{
  "name": "pharmacin",
  "version": "2.0.0",
  "description": "Pharmacy Inventory System",
  "main": "main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "web-dev": "webpack --mode development",
    "a": "tsc-watch --noClear -p ./tsconfig.json --onSuccess \"node ./out/app.js\"",
    "b": "webpack --mode production",
    "c": "electron .",
    "d": "electron-builder --x64 -w",
    "e": "git add .",
    "f": "git commit -m 'Update'",
    "g": "git push -u origin master",
    "h": "electron-builder --publish always"
  },
  "keywords": [
    "Pharmacy",
    "Stock",
    "Inventory",
    "Management",
    "Software",
    "System"
  ],
  "author": "IbnAdam",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PharmacinOnline/pharmacin.desktop.git"
  },
  "build": {
    "appId": "softmedica.app.pharmacin.desktop",
    "productName": "Pharmacin Desktop",
    "npmArgs": "--production",
    "publish": [
      {
        "provider": "github",
        "owner": "PharmacinOnline",
        "repo": "pharmacin.desktop"
      }
    ],
    "files": [
      "!out/*",
      "!src/*",
      "!.js/*",
      "!app.ts",
      "!app.js",
      "!main.min.js",
      "!tsconfig.json",
      "!webpack.config.js"
    ],
    "extraResources": [
      "build/*",
      "build/config/*",
      "print/*"
    ]
  },
  "dependencies": {
    "accesscontrol": "^2.2.1",
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "commander": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "electron-log": "^4.4.6",
    "electron-pos-printer": "^1.1.3",
    "electron-updater": "^4.6.5",
    "express": "^4.17.2",
    "express-jwt": "^5.3.1",
    "fkill": "^8.0.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "kill-port": "^1.6.1",
    "moment": "^2.24.0",
    "mongoose": "^6.2.1",
    "multer": "^1.4.4",
    "nanoid": "^3.3.2",
    "fs-extra": "^10.0.1",
    "nodejs-windows-service-controller": "^1.1.3",
    "nodemailer": "^6.4.10",
    "smpp": "^0.6.0-rc.1",
    "smtp-server": "^3.8.0",
    "socket.io": "^4.1.2",
    "socketio-file-upload": "^0.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@types/cors": "^2.8.10",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.12",
    "@types/helmet": "^4.0.0",
    "@types/node": "^14.14.41",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "babel-loader": "^8.1.0",
    "debug": "^4.3.2",
    "electron": "^18.0.3",
    "electron-builder": "^22.14.13",
    "eslint": "^7.27.0",
    "javascript-obfuscator": "^2.19.0",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.4.0",
    "typescript": "^4.3.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-node-externals": "^2.5.2",
    "winston": "^3.3.3"
  }
}
